#: .\site-packages\django 1.8\contrib\postgres\apps.py:12
msgid "PostgreSQL extensions"
msgstr "PostgreSQL 扩展。"

#: .\site-packages\django 1.8\contrib\postgres\fields\array.py:22
#: 1.8\contrib\postgres\forms\array.py:15
#: 1.8\contrib\postgres\forms\array.py:145
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "数组中的第 %(nth)s 项无法校验。"

#: .\site-packages\django 1.8\contrib\postgres\fields\array.py:23
msgid "Nested arrays must have the same length."
msgstr "嵌套数组必须是相同长度。"

#: .\site-packages\django 1.8\contrib\postgres\fields\hstore.py:15
msgid "Map of strings to strings"
msgstr "包含字符串对应字串符的映射。"

#: .\site-packages\django 1.8\contrib\postgres\fields\hstore.py:17
#, python-format
msgid "The value of \"%(key)s\" is not a string."
msgstr "“%(key)s”的值不是字符串。"

#: .\site-packages\django 1.8\contrib\postgres\forms\hstore.py:15
msgid "Could not load JSON data."
msgstr "不能加载JSON数据。"

#: .\site-packages\django 1.8\contrib\postgres\forms\ranges.py:13
msgid "Enter two valid values."
msgstr "输入两个有效的值。"

#: .\site-packages\django 1.8\contrib\postgres\forms\ranges.py:14
msgid "The start of the range must not exceed the end of the range."
msgstr "区间开头不能超过区间结尾。"

#: .\site-packages\django 1.8\contrib\postgres\forms\ranges.py:59
msgid "Enter two whole numbers."
msgstr "输入两个整数。"

#: .\site-packages\django 1.8\contrib\postgres\forms\ranges.py:65
msgid "Enter two numbers."
msgstr "输入两个数字。"

#: .\site-packages\django 1.8\contrib\postgres\forms\ranges.py:71
msgid "Enter two valid date/times."
msgstr "输入两个有效的日期／时间。"

#: .\site-packages\django 1.8\contrib\postgres\forms\ranges.py:77
msgid "Enter two valid dates."
msgstr "输入两个有效日期。"

#: .\site-packages\django 1.8\contrib\postgres\validators.py:14
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than %"
"(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than %"
"(limit_value)d."
msgstr[0] "列表已包含 %(show_value)d 项，不应该超过 %(limit_value)d 项。"

#: .\site-packages\django 1.8\contrib\postgres\validators.py:21
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than %"
"(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than %"
"(limit_value)d."
msgstr[0] "列表已包含 %(show_value)d 项，不应该少于 %(limit_value)d 项。"

#: .\site-packages\django 1.8\contrib\postgres\validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "某些键缺失：%(keys)s"

#: .\site-packages\django 1.8\contrib\postgres\validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "包含未知的键：%(keys)s"

#: .\site-packages\django 1.8\contrib\postgres\validators.py:73
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "请确保该区间内所有值少于或等于 %(limit_value)s 。"

#: .\site-packages\django 1.8\contrib\postgres\validators.py:78
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to %(limit_value)"
"s."
msgstr "请确保该区间内所有值大于或等于 %(limit_value)s 。"
